Options -Indexes
RewriteEngine Off

<IfModule mod_php.c>
    php_flag engine off
</IfModule>

<FilesMatch "\\.(php|php[0-9]+)$">
    Require all denied
</FilesMatch>

<IfModule mod_headers.c>
    Header set Cache-Control "public, max-age=31536000, immutable"
    Header set Access-Control-Allow-Origin "*"
</IfModule>

<IfModule mod_mime.c>
    RemoveHandler .php
    RemoveType .php
</IfModule>
